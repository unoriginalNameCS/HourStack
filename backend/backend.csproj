    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>a15fd4c8-aa79-4fca-96e3-cc54920e0dcc</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="10.0.3">
